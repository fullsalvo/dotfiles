! Fonts
URxvt.font:xft:ProfontIIx:pixelsize=9:antialias=True,xft:JackeyFont:pixelsize=9:antialias=True
URxvt.boldFont:
XTerm.font:xft:ProfontIIx:pixelsize=9:antialias=True,xft:JackeyFont:pixelsize=9:antialias=True
XTerm.boldFont:

! General
*.scrollBar: false
*.saveLines: 8192
*.internalBorder: 10
*.cursorUnderline: true
*.cursorBlink: true
*.dynamicColors:on
*.letterSpace: 0

Xft.autohint: 0
Xft.dpi: 96
Xft.hintstyle: hintnone
Xft.lcdfilter: lcddefault
Xft.rgba: rgb

! Rofi
rofi.opacity: 100
rofi.width: 40
rofi.lines: 3
rofi.font: ProfontIIx 8
rofi.padding: 50
rofi.bw: 5
rofi.terminal: urxvt
rofi.color-window: #18292e, #78aaa4
rofi.color-normal: #18292e, #eeeeee, #18292e, #78aaa4, #18292e
rofi.color-active: #18292e, #eeeeee, #18292e, #9bdcd3, #18292e
rofi.color-urgent: #18292e, #eeeeee, #18292e, #b79d7d, #18292e

! Clickable Links
*perl-ext-common:	 default,matcher,clipboard
*keysym.Shift-Control-V: perl:clipboard:paste
*url-launcher:		 /usr/bin/vivaldi
*matcher.button:	 1
*url-select.underline:	 true
*.matcher.rend.0:        Uline Bold fg4

! Extra

*.color254:     #18292e
*.color255:     #eeeeee

! Colors
*.borderColor:  #18292e
*.background:   #18292e
*.foreground:   #eeeeee
*.cursorColor:  #eeeeee

