nnoremap j 10j
nnoremap k 10k
inoremap <C-a> <Insert><C-a><Insert>
cnoremap <S-Insert> <C-v>
inoremap <S-Insert> <C-v>
nnoremap t t<CR>
nnoremap T t
set hintchars=asdfhjkl

highlight ContentSeparator border: none;
highlight CmdLine background: #18292e; color: #eeeeee;
highlight StatusLine background: #18292e; color: #eeeeee; font-family: ProfontIIx;
highlight Normal background: #18292e; color: #eeeeee;
highlight ModeMsg background: #78aaa4; color: #eeeeee; font-family: ProfontIIx;
highlight CmdLine>* font-family: "ProfontIIx";
highlight Hint font-family: "ProfontIIx"; background: #eeeeee; color: #18292e;
highlight HintActive background-color: #ea7c67;
highlight HintElem background-color: #b79d7d;
highlight InfoMsg background-color: #18292e; color: #ea7c67; font-family: "ProfontIIx";

iunmap <C-q>
nnoremap <C-q> :js toggle_bottombar()<CR>

:js << EOF
function toggle_bottombar(p) {
	 var bb = document.getElementById('liberator-bottombar');
	 if (!bb)
		 return;
	if (p == 'on'){
		bb.style.height = '';
		bb.style.overflow = '';
		return;
	}
	if (p == 'off'){
		bb.style.height = '0px';
		bb.style.overflow = 'hidden';
		return;
	  }

	 bb.style.height = (bb.style.height == '') ? '0px' : '';
	 bb.style.overflow = (bb.style.height == '') ? '' : 'hidden';
}
toggle_bottombar();
EOF